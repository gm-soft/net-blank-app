{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "Company.Core.Api"
    },
    "WriteToFileAllowed": true
  },
  "AllowedHosts": "*",
  "IdentityServer": {
    "Audience": "api"
  },
  "AllowedDomainEmails": {
    "Domains": "gmail.com"
  },
  "SendGridApiKey": "send-grid-secret-key",
  "BaseUrl": "https://example.com/",
  "ImageBaseUrl": "https://example.com/"
}
