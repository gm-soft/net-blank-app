{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Properties": {
      "Application": "Core.Api"
    },
    "WriteToFileAllowed": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={DATABASE_HOST};Port={DATABASE_PORT};Database={DATABASE_PROD};User Id={DATABASE_USER};Password={DATABASE_PASS}"
  },
  "IdentityServer": {
    "Authority": "https://identity.example.com"
  },
  "BaseUrl": "https://example.com",
  "ImageBaseUrl": "https://api.example.com"
}
